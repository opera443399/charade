#: fields.py:26
#, python-format
msgid "Password too short (must be %s characters or more)"
msgstr "密码长度不符合需求，至少包含 %s 个字符"

#: fields.py:30
#, python-format
msgid "Password too long (must be %s characters or less)"
msgstr "密码长度不符合需求，最多允许 %s 个字符"

#: fields.py:36
#, python-format
msgid "Weak password, %s"
msgstr "弱密码，%s"

#: fields.py:61
#, python-format
msgid "must contain %(UPPER)s or more uppercase characters (A-Z)"
msgstr "请至少包含 %(UPPER)s  个大写字符 (A-Z)"

#: fields.py:66
#, python-format
msgid "Must contain %(LOWER)s or more lowercase characters (a-z)"
msgstr "请至少包含 %(LOWER)s 个小写字符 (a-z)"

#: fields.py:71
#, python-format
msgid "must contain %(DIGITS)s or more numbers (0-9)"
msgstr "请至少包含 %(DIGITS)s 个数字 (0-9)。"

#: fields.py:76
#, python-format
msgid "must contain %(PUNCTUATION)s or more symbols"
msgstr "请至少包含 %(PUNCTUATION)s 个符号。"

#: fields.py:82
#, python-format
msgid "Sorry, %s emails are not allowed. Please use a different email address."
msgstr "抱歉，提供的 email 地址 %s 不可用，请尝试其他 email 地址"

